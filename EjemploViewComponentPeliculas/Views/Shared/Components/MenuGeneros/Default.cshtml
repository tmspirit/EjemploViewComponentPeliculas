@using EjemploViewComponentPeliculas.Models
@model List<Genero>
@{
    ViewData["Title"] = "Default";
}

<h2>Generos</h2>

<ul>
    @foreach (Genero gen in Model)
    {
        <li>
            <a asp-controller="Home" asp-action="Index" asp-route-genero="@gen.IdGenero">@gen.GeneroPelicula</a>
        </li>
    }
</ul>
